	START	100
	LDL	#-1
	LDS	#3	.INITIALIZE REGISTER S TO 3
	LDT	#31	.INITIALIZE REGISTER T TO 31
	LDX	#0
LOOP	LDA	ALPHA,X	.LOAD WORD FROM ALPHA INTO REGISTER X
	MULR	L,X	.MULTIPLY REGISTER L AND X
	STA	BETA,X	.STORE WORD IN BETA
	MULR	L,X
	ADDR	S,X
	COMPR	X,T	.COMPARE NEW X INDEX VALUE TO 31
	JLT	LOOP	.LOOP IF INDEX GREATER THAN 31
	RSUB
ALPHA	WORD	93
	WORD	50
	WORD	197
	WORD	55
	WORD	96
	WORD	65
	WORD	74
	WORD	92
	WORD	43
	WORD	8
	RESW	9
BETA	RESW	1
	END
